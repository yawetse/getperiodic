<div class="page-header">
    <h1><%= periodical.title %></h1>
</div>


<table class="table table-bordered">
    <tbody>
        <tr><th>description</th><td><%= periodical.description %></td></tr>
        <tr><th>createdAt</th><td><%= periodical.createdAt %></td></tr>
        <tr><th>collaborators</th><td><%= periodical.options %></td></tr>    
    </tbody>
</table>

<div class="well">
    <%- link_to('<i class="icon-edit icon-white"></i> Edit', path_to.edit_periodical(periodical), {class: 'btn btn-primary'}) %>
    <%- link_to('<i class="icon-remove icon-white"></i> Delete', path_to.periodical(periodical), {class: 'btn btn-danger', method: 'delete', remote: true, jsonp: '(function (u) { location.href = u; })'}) %>
    or
    <%- link_to('Back to index', path_to.periodicals()) %>
</div>