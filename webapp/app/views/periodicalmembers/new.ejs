<div class="page-header">
    <h1>New periodicalmember</h1>
</div>

<% form_for(periodicalmember, {action: path_to.periodicalmembers(), method: 'POST', id: "periodicalmember_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('periodicalmembers/form.ejs', {locals: {form: form, periodicalmember: periodicalmember}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create periodicalmember', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.periodicalmembers(), {class: 'btn'}) %>
    </div>
<% });%>
