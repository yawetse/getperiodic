<div class="page-header">
    <h1>Register for an account</h1>
</div>

<% var form = form_for(user, {action: path_to.users(), method: 'POST', id: "user_form", class: 'form-horizontal'})  %>
<%- form.begin() %>
    <%- include _register_form.ejs %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Create user', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.users(), {class: 'btn'}) %>
    </div>
<%- form.end() %>