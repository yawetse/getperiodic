<div class="page-header"><h1>Complete your registration</h1></div>

<% var form = form_for(user, {action: path_to.updateinfo(), method: 'POST', id: "user_form", class: 'form-horizontal'})  %>
<%- form.begin() %>
    <%- include _updateinfo_form.ejs %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Update user', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.user(user), {class: 'btn'}) %>
    </div>
<%- form.end() %>